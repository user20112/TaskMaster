<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage
    x:Class="TaskMasterClient.Pages.Base.MainFlyoutPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TaskMasterClient.Pages.Base"
    xmlns:viewModelBase="clr-namespace:TaskMasterClient.ViewModels.Base"
    xmlns:base="clr-namespace:TaskMasterClient.Pages.Base"
    xmlns:pages="clr-namespace:TaskMasterClient.Pages" >
    <FlyoutPage.Flyout>
        <local:DrawerPage
            Title="Menu" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <base:CustomNavigationPage>
            <x:Arguments>
                <pages:HomePage />
            </x:Arguments>
        </base:CustomNavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>